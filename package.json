{
  "name": "bike-fitting-booking",
  "version": "1.0.0",
  "description": "Application de réservation de créneaux d'étude posturale (bike fitting)",
  "private": true,
  
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently --kill-others \"npm:dev:backend\" \"npm:dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build --workspace=backend && npm run build --workspace=frontend",
    "start": "npm run start --workspace=backend",
    "docs:jsdoc": "jsdoc -c jsdoc.conf.json",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2"
  }

  
}
